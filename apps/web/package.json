{
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:full": "concurrently \"npm run dev\" \"npm run stripe:listen\"",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "db:generate": "npx dotenv-cli -e .env.local -- drizzle-kit generate",
    "db:generate:custom": "npx dotenv-cli -e .env.local -- drizzle-kit generate --custom",
    "db:migrate": "npx dotenv-cli -e .env.local -- tsx scripts/migrate.ts",
    "db:migrate:prod": "npx dotenv-cli -e .env.prod -- tsx scripts/migrate.ts",
    "db:rollback": "npx dotenv-cli -e .env.local -- tsx scripts/rollback.ts",
    "db:rollback:prod": "npx dotenv-cli -e .env.prod -- tsx scripts/rollback.ts",
    "db:status": "npx dotenv-cli -e .env.local -- tsx scripts/migration-status.ts",
    "db:status:prod": "npx dotenv-cli -e .env.prod -- tsx scripts/migration-status.ts",
    "db:seed": "npx dotenv-cli -e .env.local -- tsx scripts/seed.ts",
    "storage:setup": "npx dotenv-cli -e .env.local -- tsx scripts/setup-storage.ts",
    "env:pull:dev": "vercel env pull .env.local --environment=preview",
    "env:pull:prod": "vercel env pull .env.prod --environment=production"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.11",
    "@ai-sdk/react": "^2.0.22",
    "@google-cloud/aiplatform": "^5.6.0",
    "@google-cloud/storage": "^7.17.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "latest",
    "@t3-oss/env-nextjs": "^0.13.8",
    "ai": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "lucide-react": "^0.516.0",
    "next": "latest",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-social-icons": "^6.24.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.5",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
